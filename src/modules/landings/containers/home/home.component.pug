        topNavClasses='bg-transparent navbar-light fixed-top',
        brandClasses='text-primary',
        buttonClasses='btn-primary'
    )

    sbpro-page-header(
        headerClasses='bg-white page-header-ui-light',
        borderType='rounded',
        borderClasses='text-light'
    )
