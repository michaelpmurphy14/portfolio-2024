        topNavClasses='bg-transparent navbar-dark fixed-top',
        brandClasses='text-white',
        buttonClasses='btn-teal'
    )

    sbpro-page-header(
        headerClasses='bg-gradient-primary-to-secondary page-header-ui-dark',
        borderType='rounded',
